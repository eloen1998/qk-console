{
  "name": "qk-console",
  "displayName": "qkConsole",
  "description": "A extension for quickly console.",
  "publisher": "eloen",
  "author": "eloen",
  "repository": "https://github.com/eloen1998/qk-console",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qkConsole.insertConsole",
        "title": "快速插入打印语句"
      }
    ],
    "keybindings": [
      {
        "command": "qkConsole.insertConsole",
        "mac": "cmd+y"
      }
    ],

    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      },
      {
        "language": "vue",
        "path": "./snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test:unit": "vitest --run"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.18.3",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "8.22.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@babel/parser": "^7.20.7",
    "@babel/traverse": "^7.20.12",
    "@babel/types": "^7.20.7",
    "@vue/compiler-sfc": "^3.2.45",
    "vitest": "^0.27.2"
  }
}