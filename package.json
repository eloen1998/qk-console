{
    "name": "qk-console",
    "displayName": "qkConsole",
    "description": "A extension for quickly console.",
    "publisher": "eloen",
    "author": "eloen",
    "repository": {
        "type": "git",
        "url": "https://github.com/eloen1998/qk-console"
    },
    "version": "0.3.0",
    "icon": "static/icon.png",
    "engines": {
        "vscode": ">=1.70.0"
    },
    "categories": [
        "Other",
        "Snippets"
    ],
    "main": "./dist/extension.js",
    "activationEvents": [],
    "contributes": {
        "commands": [
            {
                "command": "qkConsole.insertConsole",
                "title": "插入打印语句"
            },
            {
                "command": "qkConsole.deleteConsole",
                "title": "删除所有打印语句"
            }
        ],
        "keybindings": [
            {
                "command": "qkConsole.insertConsole",
                "mac": "cmd+y"
            },
            {
                "command": "qkConsole.deleteConsole",
                "mac": "cmd+u"
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets.json"
            },
            {
                "language": "vue",
                "path": "./snippets.json"
            },
            {
                "language": "typescript",
                "path": "./snippets.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "qkConsole",
            "properties": {
                "qkConsole.singleQuote": {
                    "type": "boolean",
                    "default": true,
                    "description": "使用单引号而不是双引号"
                },
                "qkConsole.semi": {
                    "type": "boolean",
                    "default": true,
                    "description": "结尾分号"
                },
                "qkConsole.delete.types": {
                    "type": "array",
                    "default": [
                        "log"
                    ],
                    "description": "需要删除的console类型"
                },
                "qkConsole.prefix": {
                    "type": "string",
                    "default": "",
                    "description": "打印语句前缀"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn package",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test:unit": "vitest --run",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/babel__traverse": "7.18.3",
        "@types/glob": "8.0.0",
        "@types/mocha": "10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "1.70.0",
        "@typescript-eslint/eslint-plugin": "5.45.0",
        "@typescript-eslint/parser": "5.45.0",
        "@vscode/test-electron": "2.2.0",
        "eslint": "8.22.0",
        "glob": "8.0.3",
        "mocha": "10.1.0",
        "ts-loader": "^9.4.3",
        "typescript": "4.9.3",
        "vite": "4.0.4",
        "webpack": "^5.84.1",
        "webpack-cli": "^5.1.1"
    },
    "dependencies": {
        "@babel/parser": "7.20.7",
        "@babel/traverse": "7.20.12",
        "@babel/types": "7.20.7",
        "@vue/compiler-sfc": "3.2.45",
        "vitest": "0.27.2"
    }
}